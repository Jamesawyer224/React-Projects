{

    "extends":[
        "eslint:recommended",
        "plugin:import/errors",
        "piugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "prettier"],
    "rules":{
        // "react/prop-types":"off"s
    },    
    "plugins":[],
    "parserOptions":{
        "ecmaVersion":2022,
        "sourceType":"module",
        "ecmaFeatures":{
            "jsx":true
        }
    },
    "env":{
        "es6":true,
        "browser":true,
        "node":true
    },

    "settings": {
        "react":{
            "version":"detect"
        },
        "import/resolver":{
            "node":{
                "extensions":[".js",".jsx"]
            }
        }
    }
}
